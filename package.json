    "@types/react": "16.4.14",
    "@types/react-icons": "2.2.6",
    "@types/styled-components": "3.0.1",
    "docz": "0.12.5",
    "enzyme": "3.6.0",
    "enzyme-adapter-react-16": "1.5.0",
    "jest": "23.6.0",
    "prettier": "1.14.3",
    "react": "16.5.2",
